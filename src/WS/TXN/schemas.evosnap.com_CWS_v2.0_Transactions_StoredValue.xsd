							<xs:appinfo/>
							<xs:documentation>
								<summary> Aggregate used to submit account number length and BIN number for virtual card activation. Conditional, for virtual card activation when TxnCode = 'Issue_Account'. At least one of CardData, VirtualCardData, CardDataToken or SecureCardData must be provided. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="StoredValueActivateTenderData" type="tns:StoredValueActivateTenderData" nillable="true"/>
	<xs:complexType name="VirtualCardData">
		<xs:annotation>
			<xs:appinfo/>
			<xs:documentation>
				<summary> Aggregate used to submit account number length and BIN number for virtual card activation. Conditional, for virtual card activation when TxnCode = 'Issue_Account'. At least one of CardData, VirtualCardData, CardDataToken or SecureCardData must be provided. </summary>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AccountNumberLength" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>
						<summary> The length of the account number that will be returned by the service provider. Required. </summary>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BIN" type="xs:string" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>
						<summary> The virtual gift card BIN number assigned by the service provider. Required. </summary>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="VirtualCardData" type="tns:VirtualCardData" nillable="true"/>
	<xs:complexType name="StoredValueManage" mixed="false">
		<xs:annotation>
			<xs:appinfo/>
			<xs:documentation>
				<summary> Stored Value udpdate data for building Deactivate, Status, Reload and BalanceTransfer from Activate. </summary>
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="q5:Manage">
				<xs:sequence>
					<xs:element name="Amount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> The amount to be reloaded on the card. Conditional required if processing Reload. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SourceCardData" type="tns:CardData" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Card data for the source account. Conditional, required if processing Balance Transfer. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardStatus" type="tns:CardStatus" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Specifies card status for status update. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsCashOut" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Specifies cash out deactivation. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OperationType" type="tns:OperationType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Indicates the opertion type for ManageAccount. Conditional, required if processing ManageAccount. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="StoredValueManage" type="tns:StoredValueManage" nillable="true"/>
	<xs:complexType name="StoredValueReturn" mixed="false">
		<xs:complexContent>
			<xs:extension base="q6:Return">
				<xs:sequence>
					<xs:element name="Amount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Amount to return if different from the authorized amount. Conditional, required when returning a different amount than authorized. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="StoredValueReturn" type="tns:StoredValueReturn" nillable="true"/>
	<xs:complexType name="StoredValueCapture" mixed="false">
		<xs:complexContent>
			<xs:extension base="q7:Capture">
				<xs:sequence>
					<xs:element name="Amount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Amount to capture if different from the authorized amount. Conditional, required when capturing a different amount than authorized. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="StoredValueCapture" type="tns:StoredValueCapture" nillable="true"/>
	<xs:complexType name="StoredValueTransactionResponse" mixed="false">
		<xs:annotation>
			<xs:appinfo/>
			<xs:documentation>
				<summary> Response to the Stored Value transaction (gift card). Expected. </summary>
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="q8:Response">
				<xs:sequence>
					<xs:element name="Amount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Specifies the authorization amount of the transaction. This is the actual amount authorized and should be used in settlement instead of the request amount. Required. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FeeAmount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Indicates fees charged to process transaction if applicable. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApprovalCode" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> A value returned when a transaction is approved. Required. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CVResult" type="q8:CVResult" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Response code returned by the card issuer indicating the result of Card Verification (CVV2/CVC2/CID). Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CashBackAmount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> The amount to return to the cardholder in another tender. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LockAmount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> The value on the card that is locked due to balance lock transaction received. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NewBalance" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> The account balance after the transaction. Required. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PreviousBalance" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> The account balance prior to the transaction. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardStatus" type="tns:CardStatus" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Indicates the status of the card if one is present. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AccountNumber" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Virtual card account number. Conditional, required for virtual card activation. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CVData" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> CVD2 data (gift card security code) for virtual card activations. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardRestrictionValue" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Specifies the gift card restriction value assigned to a specific card. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaymentAccountDataToken" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Token specified by a tokenization (offsite payment account data storage) Service Provider, used to retrieve securely stored payment account data. Conditional, required if any one of the following must be provided: CardDataToken, SecureCardData, or tender-specific payment account data. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaskedPAN" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Masked PAN for storage. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OrderId" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> The order id generated by CWS. Often used by service providers for transaction correlation. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Expire" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Card expiration. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="StoredValueTransactionResponse" type="tns:StoredValueTransactionResponse" nillable="true"/>
	<xs:complexType name="StoredValueCaptureResponse" mixed="false">
		<xs:annotation>
			<xs:appinfo/>
			<xs:documentation>
				<summary> Response to capturing one or more Stored Value transactions. Expected. </summary>
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="q9:Response">
				<xs:sequence>
					<xs:element name="BatchId" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Batch Identifier. Optional. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SummaryData" type="q9:SummaryData" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:appinfo/>
							<xs:documentation>
								<summary> Summary information for Stored Value totals. Required. </summary>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="StoredValueCaptureResponse" type="tns:StoredValueCaptureResponse" nillable="true"/>
</xs:schema>
